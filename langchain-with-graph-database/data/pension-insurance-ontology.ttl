  rdfs:range pen:Member .

pen:benefitDerivedFromPlan a rdf:Property ;
  rdfs:label "benefit derived from plan" ;
  rdfs:domain pen:Benefit ;
  rdfs:range pen:PensionPlan .

pen:hasBenefitCalculation a rdf:Property ;
  rdfs:label "has benefit calculation" ;
  rdfs:domain pen:Benefit ;
  rdfs:range pen:BenefitCalculation .

pen:calculationDate a rdf:Property ;
  rdfs:label "calculation date" ;
  rdfs:domain pen:BenefitCalculation ;
  rdfs:range xsd:date .

pen:calculationMethod a rdf:Property ;
  rdfs:label "calculation method" ;
  rdfs:domain pen:BenefitCalculation ;
  rdfs:range xsd:string .

pen:calculatedAmount a rdf:Property ;
  rdfs:label "calculated amount" ;
  rdfs:domain pen:BenefitCalculation ;
