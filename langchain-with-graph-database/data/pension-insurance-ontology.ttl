pen:claimant a rdf:Property ;
  rdfs:label "claimant" ;
  rdfs:domain pen:Claim ;
  rdfs:range pen:Party .

pen:hasClaimDecision a rdf:Property ;
  rdfs:label "has claim decision" ;
  rdfs:domain pen:Claim ;
  rdfs:range pen:ClaimDecision .

pen:decisionStatus a rdf:Property ;
  rdfs:label "decision status" ;
  rdfs:domain pen:ClaimDecision ;
  rdfs:range xsd:string .

pen:decisionDate a rdf:Property ;
  rdfs:label "decision date" ;
  rdfs:domain pen:ClaimDecision ;
  rdfs:range xsd:date .

pen:paysBenefit a rdf:Property ;
  rdfs:label "pays benefit" ;
  rdfs:domain pen:Payment ;
  rdfs:range pen:Benefit .

pen:paymentTo a rdf:Property ;
  rdfs:label "payment to" ;
  rdfs:domain pen:Payment ;
  rdfs:range pen:Party .

pen:paymentAmount a rdf:Property ;
  rdfs:label "payment amount" ;
  rdfs:domain pen:Payment ;
  rdfs:range xsd:decimal .

pen:paymentDate a rdf:Property ;
